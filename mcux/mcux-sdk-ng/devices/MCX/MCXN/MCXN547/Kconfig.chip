    default "MCXN547VDF" if MCUX_HW_DEVICE_PART_MCXN547VDF
    default "MCXN547VPB" if MCUX_HW_DEVICE_PART_MCXN547VPB
    default "MCXN547VNL" if MCUX_HW_DEVICE_PART_MCXN547VNL

