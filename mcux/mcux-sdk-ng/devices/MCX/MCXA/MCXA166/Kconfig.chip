# device related

config MCUX_HW_DEVICE_ID_MCXA166
    bool

choice
    prompt "Device MCXA166 Part"
    config MCUX_HW_DEVICE_PART_MCXA166VLQ
    bool "Device part MCXA166VLQ"
    select MCUX_HW_DEVICE_ID_MCXA166
    config MCUX_HW_DEVICE_PART_MCXA166VLL
    bool "Device part MCXA166VLL"
    select MCUX_HW_DEVICE_ID_MCXA166
    config MCUX_HW_DEVICE_PART_MCXA166VLH
    bool "Device part MCXA166VLH"
    select MCUX_HW_DEVICE_ID_MCXA166
    config MCUX_HW_DEVICE_PART_MCXA166VPN
    bool "Device part MCXA166VPN"
    select MCUX_HW_DEVICE_ID_MCXA166
endchoice

config MCUX_HW_DEVICE_ID
    string
    default "MCXA166" if MCUX_HW_DEVICE_ID_MCXA166

config MCUX_HW_DEVICE_PART
    string
    default "MCXA166VLQ" if MCUX_HW_DEVICE_PART_MCXA166VLQ
    default "MCXA166VLL" if MCUX_HW_DEVICE_PART_MCXA166VLL
    default "MCXA166VLH" if MCUX_HW_DEVICE_PART_MCXA166VLH
    default "MCXA166VPN" if MCUX_HW_DEVICE_PART_MCXA166VPN

