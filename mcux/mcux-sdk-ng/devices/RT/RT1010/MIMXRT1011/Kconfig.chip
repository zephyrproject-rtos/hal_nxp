config MCUX_HW_SOC_SERIES_RT
    bool
    default y

config MCUX_HW_SOC_SINGLECORE_DEVICE
    bool
    default y

# device related
config MCUX_HW_DEVICE_ID_MIMXRT1011xxxxx
    bool

choice
    prompt "Device MIMXRT1011 Part"
    config MCUX_HW_DEVICE_PART_MIMXRT1011DAE5A
        bool "Device part MIMXRT1011DAE5A"
        select MCUX_HW_DEVICE_ID_MIMXRT1011xxxxx
    config MCUX_HW_DEVICE_PART_MIMXRT1011CAE4A
        bool "Device part MIMXRT1011CAE4A"
        select MCUX_HW_DEVICE_ID_MIMXRT1011xxxxx
endchoice

config MCUX_HW_DEVICE_ID
