# device related

config MCUX_HW_DEVICE_ID_MIMXRT555S
    bool

choice
    prompt "Device MIMXRT555S Part"
    config MCUX_HW_DEVICE_PART_MIMXRT555SFFOC
    bool "Device part MIMXRT555SFFOC"
    select MCUX_HW_DEVICE_ID_MIMXRT555S
    config MCUX_HW_DEVICE_PART_MIMXRT555SFAWC
    bool "Device part MIMXRT555SFAWC"
    select MCUX_HW_DEVICE_ID_MIMXRT555S
endchoice

config MCUX_HW_DEVICE_ID
    string
    default "MIMXRT555S" if MCUX_HW_DEVICE_ID_MIMXRT555S

config MCUX_HW_DEVICE_PART
    string
    default "MIMXRT555SFFOC" if MCUX_HW_DEVICE_PART_MIMXRT555SFFOC
    default "MIMXRT555SFAWC" if MCUX_HW_DEVICE_PART_MIMXRT555SFAWC

