config MCUX_HW_SOC_SERIES_RT
    bool
    default y

config MCUX_HW_SOC_SINGLECORE_DEVICE
    bool
    default y

# device related
config MCUX_HW_DEVICE_ID_MIMXRT1171xxxxx
    bool

choice
    prompt "Device MIMXRT1171 Part"
    config MCUX_HW_DEVICE_PART_MIMXRT1171DVMAA
        bool "Device part MIMXRT1171DVMAA"
        select MCUX_HW_DEVICE_ID_MIMXRT1171xxxxx
    config MCUX_HW_DEVICE_PART_MIMXRT1171AVM8A
        bool "Device part MIMXRT1171AVM8A"
        select MCUX_HW_DEVICE_ID_MIMXRT1171xxxxx
    config MCUX_HW_DEVICE_PART_MIMXRT1171CVM8A
        bool "Device part MIMXRT1171CVM8A"
        select MCUX_HW_DEVICE_ID_MIMXRT1171xxxxx
endchoice

config MCUX_HW_DEVICE_ID
