# driver_inputmux
config MCUX_HAS_driver_inputmux
    bool

config MCUX_USE_driver_inputmux
    depends on MCUX_HAS_driver_inputmux
    bool "INPUTMUX Driver"
    select MCUX_USE_driver_common
    select MCUX_USE_driver_inputmux_connections
    help
      INPUTMUX Driver
